import javax.swing.*;

public class RectJPN {
    public static void main(String[] args) {
        String x= JOptionPane.showInputDialog(null,"Enter the height.");
        double height=Double.parseDouble(x);
        String y = JOptionPane.showInputDialog(null,"Enter the width.");
        double width=Double.parseDouble(y);
        double area=height*width;
        JOptionPane.showMessageDialog(null,"Area = " + area);
    }
}
